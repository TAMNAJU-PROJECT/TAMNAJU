<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tamnaju.dev.domains.mappers.SignatureMapper">
    <select id="getSignatureEntity" resultType="com.tamnaju.dev.domains.entities.SignatureEntity">
        SELECT `keyByte`   AS `key_byte`,
               `createdAt` AS `created_at`
        FROM `tamnaju_db`.`signatures`
    </select>
</mapper>
